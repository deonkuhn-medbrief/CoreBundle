    "type": "package",
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "^3.4",
        "doctrine/orm": "^2.2",
        "vich/uploader-bundle": ">=0.14",
        "friendsofsymfony/message-bundle": "^2.0",
        "lexik/form-filter-bundle": "^5.0"
    },
    "license": "mit",
    "authors": [
        {
            "name": "Rowan Reid",
            "email": "rowan@sideclick.co.za"
        },
        {
            "name": "Stuart Honour",
            "email": "stuart.honour@medbrief.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MedBrief\\CoreBundle\\": "src/"
        }
    }
}
